<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SwapStactionDetailsActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background_color"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/bac"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="start"
            android:gravity="start"

            android:src="@drawable/back_foreground"/>

        <TextView
            android:id="@+id/agentname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:fontFamily="@font/ibm_bold"
            android:gravity="center"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/dp_20" />
    </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/ibm_bold"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:text="Battery Availability"
                android:textColor="@color/white"
                android:textSize="@dimen/dp_15" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:gravity="end"
                android:layout_gravity="end"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="start"
                    android:layout_marginEnd="3dp"
                    android:gravity="start"
                    android:src="@drawable/phone"/>


                <TextView
                    android:id="@+id/agentnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ibm_bold"
                    android:gravity="center"
                    android:text="000000000"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_gravity="center"
                    android:textColor="@color/white"
                    android:textSize="@dimen/dp_15" />
            </LinearLayout>


        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="15dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:orientation="horizontal">


                <LinearLayout
                    android:id="@+id/location"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_weight="1"
                    android:background="@drawable/swaping_stion"
                    android:layout_marginEnd="5dp"

                    android:layout_gravity="start"
                    android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:scaleType="fitXY"
                        app:tint="@color/white"
                        android:src="@drawable/battery_100"/>

                    <TextView
                        android:id="@+id/battery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/ibm_bold"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_weight="1"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="20"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_20" />

                </LinearLayout>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/ibm_bold"
                        android:layout_weight="0.75"
                        android:gravity="center"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_marginTop="15dp"
                        android:text="75%-100%"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_15" />
                </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:layout_marginStart="5dp"
                android:background="@drawable/swaping_stion"
                android:layout_gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:scaleType="fitXY"
                        app:tint="@color/white"
                        android:src="@drawable/battery_75"/>

                    <TextView
                        android:id="@+id/battery_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/ibm_bold"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_weight="1"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="20"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_20" />

                </LinearLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ibm_bold"
                    android:layout_weight="0.75"
                    android:gravity="center"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="15dp"
                    android:text="50%-75%"
                    android:textColor="@color/white"
                    android:textSize="@dimen/dp_15" />
            </LinearLayout>




        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"

            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@drawable/swaping_stion"
                android:layout_marginEnd="5dp"

                android:layout_gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:scaleType="fitXY"
                        app:tint="@color/white"
                        android:src="@drawable/battery_50"/>

                    <TextView
                        android:id="@+id/battery_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/ibm_bold"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_weight="1"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="20"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_20" />

                </LinearLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ibm_bold"
                    android:layout_weight="0.75"
                    android:gravity="center"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="15dp"
                    android:text="25%-50%"
                    android:textColor="@color/white"
                    android:textSize="@dimen/dp_15" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_weight="1"
                android:background="@drawable/swaping_stion"
                android:layout_marginStart="5dp"
                android:layout_gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:scaleType="fitXY"
                        app:tint="@color/white"
                        android:src="@drawable/battery_25"/>

                    <TextView
                        android:id="@+id/battery_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/ibm_bold"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_weight="1"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="20"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_20" />

                </LinearLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ibm_bold"
                    android:layout_weight="0.75"
                    android:gravity="center"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="15dp"
                    android:text="0%-25%"
                    android:textColor="@color/white"
                    android:textSize="@dimen/dp_15" />
            </LinearLayout>




        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_marginTop="20dp"

        android:layout_height="40dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:layout_marginEnd="5dp"
            android:layout_marginStart="5dp"
            android:layout_height="match_parent">

        <TextView
            android:id="@+id/f1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:visibility="gone"
            android:layout_marginEnd="5dp"
            android:text="75%-100%"
            android:textColor="@color/black"
            android:textSize="14sp" />
        <TextView
            android:id="@+id/b1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/ststin_detsil_bsck"

            android:layout_marginEnd="5dp"
            android:text="75%-100%"
            android:textColor="@color/black"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/f2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginEnd="5dp"
            android:text="50%-75%"
            android:textColor="@color/black"
            android:textSize="14sp" />
            <TextView
            android:id="@+id/b2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:visibility="gone"
            android:background="@drawable/ststin_detsil_bsck"
            android:layout_marginEnd="5dp"
            android:text="50%-75%"
            android:textColor="@color/black"
            android:textSize="14sp" />
        <TextView
            android:id="@+id/f3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginEnd="5dp"
            android:text="25%-50%"
            android:textColor="@color/black"
            android:textSize="14sp" />
            <TextView
            android:id="@+id/b3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/ststin_detsil_bsck"
            android:visibility="gone"
            android:layout_marginEnd="5dp"
            android:text="25%-50%"
            android:textColor="@color/black"
            android:textSize="14sp" />
        <TextView
            android:id="@+id/f4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:text="0%-25%"
            android:textColor="@color/black"
            android:textSize="14sp" />
            <TextView
            android:id="@+id/b4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/ibm_bold"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/ststin_detsil_bsck"
            android:visibility="gone"
            android:text="0%-25%"
            android:textColor="@color/black"
            android:textSize="14sp" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:background="#EFF2F3"

        android:layout_height="match_parent"
        android:orientation="horizontal">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/first1"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_height="match_parent">
        </androidx.recyclerview.widget.RecyclerView>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/second1"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_height="match_parent">
        </androidx.recyclerview.widget.RecyclerView>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/third1"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_height="match_parent">
        </androidx.recyclerview.widget.RecyclerView>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/fourth1"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_height="match_parent">
        </androidx.recyclerview.widget.RecyclerView>

    </LinearLayout>







    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        android:gravity="bottom"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">



        <ImageView
            android:id="@+id/dashboard1"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"


            android:src="@drawable/dasboard_hmoe"/>

        <ImageView
            android:id="@+id/map1"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@drawable/active_map"/>
        <ImageView
            android:id="@+id/batterswap"
            android:layout_width="30dp"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:scaleType="fitXY" />
        <ImageView
            android:id="@+id/swap_history1"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@drawable/dasboard_swap_history"/>
        <ImageView
            android:id="@+id/profile1"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@drawable/dasboard_profile"/>

    </LinearLayout>
    <ImageView
        android:id="@+id/batterswap4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_gravity="bottom"
        android:src="@drawable/qr_dashbord2"
        app:layout_constraintBottom_toBottomOf="parent"
        android:gravity="bottom"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>